<h2>Listing settings</h2>

<table class="table">
  <thead>
    <tr>
      <th>Ssid</th>
      <th>Password</th>
      <th>Handle</th>
      <th>Hashtag</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for setting <- @settings do %>
    <tr>
      <td><%= setting.ssid %></td>
      <td><%= setting.password %></td>
      <td><%= setting.handle %></td>
      <td><%= setting.hashtag %></td>

      <td class="text-right">
        <%= link "Show", to: setting_path(@conn, :show, setting), class: "btn btn-default btn-xs" %>
        <%= link "Edit", to: setting_path(@conn, :edit, setting), class: "btn btn-default btn-xs" %>
        <%= link "Delete", to: setting_path(@conn, :delete, setting), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New setting", to: setting_path(@conn, :new) %>
